{
  "name": "sidifa-fe",
  "description": "SI-DIFA: Sistem Informasi Difabel",
  "version": "1.0.0",
  "author": "Muhammad Hisyam Kamil",
  "homepage": "https://hisyam.net",
  "engines": {
    "node": "^18.17.0 || ^20.3.0 || >=21.0.0"
  },
  "engines-annotation": "Mostly required by sharp which needs a Node-API v9 compatible runtime",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "qwik build",
    "build.client": "vite build",
    "build.preview": "vite build --ssr src/entry.preview.tsx",
    "build.server": "vite build -c adapters/bun/vite.config.mts",
    "build.types": "tsc --incremental --noEmit",
    "deploy": "echo 'Run \"npm run qwik add\" to install a server adapter'",
    "dev": "vite --mode ssr",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "lint": "eslint \"src/**/*.ts*\"",
    "preview": "qwik build preview && vite preview --open",
    "serve": "bun server/entry.bun.js",
    "start": "vite --open --mode ssr",
    "qwik": "qwik"
  },
  "devDependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@eslint/js": "^9.35.0",
    "@modular-forms/qwik": "^0.29.1",
    "@qwik.dev/core": "^2.0.0-beta.9",
    "@qwik.dev/react": "^2.0.0-beta.9",
    "@qwik.dev/router": "^2.0.0-beta.9",
    "@qwikest/icons": "^0.0.13",
    "@tailwindcss/typography": "^0.5.16",
    "@tailwindcss/vite": "^4.1.13",
    "@types/bun": "^1.2.21",
    "@types/d3": "^7.4.3",
    "@types/node": "^24.3.3",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "daisyui": "^5.1.10",
    "dotenv": "^17.2.2",
    "eslint": "^9.35.0",
    "eslint-plugin-qwik": "^1.16.0",
    "globals": "^16.4.0",
    "install": "^0.13.0",
    "mdx-editor": "^0.0.1",
    "prettier": "^3.6.2",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "tailwindcss": "^4.1.13",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.43.0",
    "undici": "^7.16.0",
    "vite": "^7.1.5",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "dependencies": {
    "@mdxeditor/editor": "^3.45.0",
    "clsx": "^2.1.1",
    "d3": "^7.9.0",
    "dompurify": "^3.2.6",
    "marked": "^16.2.1",
    "motion": "^12.23.12",
    "valibot": "^1.1.0",
    "xior": "^0.7.8"
  }
}
